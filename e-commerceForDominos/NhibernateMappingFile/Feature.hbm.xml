<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="e_commerceForDominos.NhibernateMappingFile.Feature, e_commerceForDominos" table="[Feature]" dynamic-insert="true" dynamic-update="true" lazy="true">

    <id name="USER_ID" type="int">
      <column name="USER_ID" sql-type="int" not-null="true"/>
      <generator class="native" />
    </id>

    <property name="USER_NAME" type="String">
      <column name="USER_NAME" sql-type="VARCHAR(30)" not-null="false"/>
    </property>

    <property name="PASSWORD" type="String">
      <column name="PASSWORD" sql-type="VARCHAR(20)" not-null="false"/>
    </property>

  </class>
</hibernate-mapping>
